{
    "AllowedHosts": "*",
    "Configs": {
        "HttpPort": 9001,
        "IsRunningInContainer": true,
        "HttpsPort": 9002
    },
    "ReverseProxy": {
        "Routes": {
            "users-route": {
                "ClusterId": "users-cluster",
                "Match": {
                    "Path": "/users/{**catch-all}"
                }
            },
            "master-route": {
                "ClusterId": "master-cluster",
                "Match": {
                    "Path": "/master/{**catch-all}"
                }
            },
            "admin-route": {
                "ClusterId": "admin-cluster",
                "Match": {
                    "Path": "/admin/{**catch-all}"
                }
            }
        },
        "Clusters": {
            "users-cluster": {
                "Destinations": {
                    "users-service": {
                        "Address": "http://users-service"
                    }
                }
            },
            "master-cluster": {
                "Destinations": {
                    "master-service": {
                        "Address": "http://master-service"
                    }
                }
            },
            "admin-cluster": {
                "Destinations": {
                    "admin-service": {
                        "Address": "http://admin-service"
                    }
                }
            }
        }
    }
}
