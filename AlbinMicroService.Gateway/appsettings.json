{
    "AllowedHosts": "*",
    "Configs": {
        "HttpPort": 9001,
        "IsRunningInContainer": true,
        "HttpsPort": 9002
    },
    "ReverseProxy": {
        "Routes": {
            "users-api": {
                "ClusterId": "users-cluster",
                "Match": {
                    "Path": "/users/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/users" }
                ]
            },
            "users-swagger": {
                "ClusterId": "users-cluster",
                "Match": {
                    "Path": "/users/swagger/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/users/swagger" }
                ]
            },
            "master-api": {
                "ClusterId": "master-cluster",
                "Match": {
                    "Path": "/master/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/master" }
                ]
            },
            "master-swagger": {
                "ClusterId": "master-cluster",
                "Match": {
                    "Path": "/master/swagger/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/master/swagger" }
                ]
            },
            "admin-api": {
                "ClusterId": "admin-cluster",
                "Match": {
                    "Path": "/admin/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/admin" }
                ]
            },
            "admin-swagger": {
                "ClusterId": "admin-cluster",
                "Match": {
                    "Path": "/admin/swagger/{**remainder}"
                },
                "Transforms": [
                    { "PathRemovePrefix": "/admin/swagger" }
                ]
            }
        },
        "Clusters": {
            "users-cluster": {
                "Destinations": {
                    "users-service": {
                        "Address": "http://localhost:8001/"
                    }
                }
            },
            "master-cluster": {
                "Destinations": {
                    "master-service": {
                        "Address": "http://localhost:8003/"
                    }
                }
            },
            "admin-cluster": {
                "Destinations": {
                    "admin-service": {
                        "Address": "http://localhost:8005/"
                    }
                }
            }
        }
    }
}
